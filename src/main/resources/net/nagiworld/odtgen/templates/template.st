<?xml version="1.0" encoding="UTF-8"?>
<table xmlns="http://query.yahooapis.com/v1/schema/table.xsd">
    <meta>
        <author>$author$</author>
        <documentationURL>$documentationURL$</documentationURL>
        <sampleQuery>$sampleQuery$</sampleQuery>
    </meta>
    <bindings>
        <$binding$ itemPath="$itemPath$" produces="XML">
            <urls>
                <url></url>
            </urls>
$if(paging)$<paging model="offset">
              <start id="start" default="0" />
              <pagesize id="count" max="50" />
              <total default="10"/>
            </paging>$endif$
            <inputs>
                $keys:{<key id="$it.name$" type="$it.type$" required="$it.required$"/>};separator="\n"$
                <key id="query" type="xs:string" paramType="variable"/>
            </inputs>
            <execute><![CDATA[
                response.object = <item><title>Simple Example generated by ODT Generator</title></item>;
            ]]></execute>
        </$binding$>
    </bindings>
</table>
